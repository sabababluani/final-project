  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "postinstall": "husky install || true",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --require ts-node/register --test src/**/*.spec.ts",
    "test:watch": "node --require ts-node/register --test --watch src/**/*.spec.ts",
    "test:integration": "node --require ts-node/register --test src/**/*.integration.spec.ts",
    "test:e2e": "node --require ts-node/register --test test/**/*.e2e-spec.ts",
    "test:cov": "c8 --reporter=html --reporter=text --reporter=lcov node --require ts-node/register --test src/**/*.spec.ts",
    "test:cov:watch": "c8 node --require ts-node/register --test --watch src/**/*.spec.ts",
    "test:cov:integration": "c8 --reporter=html --reporter=text --reporter=lcov node --require ts-node/register --test src/**/*.integration.spec.ts",
    "test:cov:e2e": "c8 --reporter=html --reporter=text --reporter=lcov node --require ts-node/register --test test/**/*.e2e-spec.ts",
    "test:cov:all": "c8 --reporter=html --reporter=text --reporter=lcov --all node --require ts-node/register --test src/**/*.spec.ts src/**/*.integration.spec.ts test/**/*.e2e-spec.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "disconnect": "^1.2.2",
    "express-session": "^1.18.2",
    "husky": "^9.1.7",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^19.1.0",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "typeorm": "^0.3.27",
    "@nestjs/cli": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.2",
    "c8": "^10.1.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
